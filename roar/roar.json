  } ]
}, {
  "@id" : "https://w3id.org/roar#PersonReconstruction",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "PersonReconstruction"
