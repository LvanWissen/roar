    <!-- http://www.w3.org/ns/prov#Activity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#qualifiedAssociation"/>
