                                                  rdfs:label \"burried\" ] ;
                                   rdf:value _:person1 ] ;
                    sem:hasTimeStamp \"1697-02-28\"^^xsd:date ] ;
        bio:birth [ a bio:Birth ;
                    bio:principal _:person1 ;
                    sem:hasActor [ a sem:Role ;
                                   sem:roleType [ a sem:RoleType ;
                                                  rdfs:label \"born\" ] ;
